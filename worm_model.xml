  <option timestep="0.002" gravity="0 0 -9.81" integrator="implicit">
    <flag warmstart="enable"/>
  </option>
  <!-- Low gravity to keep worm grounded, swimming approximated by low friction -->

  <default>
    <joint damping="0.5" stiffness="0.1" armature="0.01" limited="true"/>
    <geom type="capsule" size="0.02" rgba="0.9 0.7 0.5 1" friction="0.1 0.01 0.001" density="1000"/>
    <motor ctrllimited="true" ctrlrange="-1 1" gear="0.1"/>
    <site size="0.005" rgba="1 0 0 0.5"/>
  </default>

